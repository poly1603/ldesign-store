{
  "name": "@ldesign/store",
  "version": "0.1.0",
  "description": "一个基于Pinia的Vue3状态管理库，支持类、Hook、Provider、装饰器等多种使用方式，性能优越，功能丰富",
  "keywords": [
    "pinia",
    "vue3",
    "state-management",
    "store",
    "typescript",
    "decorators",
    "hooks",
    "provider",
    "reactive",
    "composition-api"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs",
      "default": "./es/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output",
    "size-check": "size-limit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:check": "pnpm run build && node ../../tools/scripts/build/bundle-validator.js",
    "build:analyze": "pnpm run build && node ../../tools/scripts/build/bundle-analyzer.js",
    "build:validate": "pnpm run build && node ../../tools/scripts/build/validate-build.js",
    "build:browser-test": "pnpm run build && node ../../tools/scripts/build/browser-tester.js",
    "build:dev": "node scripts/build.js --dev",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@types/ws": "^8.5.10",
    "reflect-metadata": "^0.2.1",
    "tslib": "^2.6.2",
    "ws": "^8.18.3"
  },
  "peerDependencies": {
    "pinia": "^2.1.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@playwright/test": "^1.40.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5",
    "@antfu/eslint-config": "^6.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "40 KB"
    }
  ],
  "peerDependenciesMeta": {
    "pinia": {
      "optional": false
    },
    "vue": {
      "optional": true
    }
  },
  "typings": "./es/index.d.ts",
  "browser": "./dist/index.js"
}