{
  "name": "@ldesign/store-example-unified",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "@ldesign/store 统一示例项目 - 展示所有核心功能和使用场景",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:performance": "vitest tests/performance --reporter=verbose",
    "test:performance:ui": "vitest tests/performance --ui",
    "test:benchmark": "vitest tests/performance/benchmark.test.ts",
    "test:benchmark:baseline": "vitest tests/performance/benchmark.test.ts --reporter=json --outputFile=tests/performance/baseline.json",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@ldesign/store": "workspace:*",
    "pinia": "^2.1.7",
    "reflect-metadata": "^0.2.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/tsconfig": "^0.5.1",
    "benchmark": "^2.1.4",
    "eslint": "^9.0.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^2.0.0",
    "vue-tsc": "^3.0.5"
  }
}
