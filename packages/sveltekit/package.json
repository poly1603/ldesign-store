{
  "name": "@ldesign/store-sveltekit",
  "version": "0.1.0",
  "description": "SvelteKit adapter for @ldesign/store - extends Svelte adapter with SvelteKit specific features",
  "keywords": [
    "sveltekit",
    "svelte",
    "state-management",
    "store",
    "ssr"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts"
  },
  "dependencies": {
    "@ldesign/store-core": "workspace:*",
    "@ldesign/store-svelte": "workspace:*"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "typescript": "^5.7.3"
  }
}



