{
  "name": "@ldesign/store-nuxtjs",
  "version": "0.1.0",
  "description": "Nuxt.js adapter for @ldesign/store - extends Vue adapter with Nuxt.js specific features",
  "keywords": [
    "nuxtjs",
    "vue",
    "state-management",
    "store",
    "ssr"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts"
  },
  "dependencies": {
    "@ldesign/store-core": "workspace:*",
    "@ldesign/store-vue": "workspace:*"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0",
    "vue": "^3.3.0",
    "pinia": "^2.1.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "typescript": "^5.7.3"
  }
}



